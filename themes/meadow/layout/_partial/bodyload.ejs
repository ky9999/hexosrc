<%# 不蒜子计数 %>
<%if (theme.busuanzi.site || theme.busuanzi.page){%>
    <script async src="<%- theme.busuanzi.busuanzi_js %>"></script>
<%}%>

<%# Mermaid %>
<% if (theme.mermaid.enable && is_post()) { %>
    <script src="https://cdn.jsdelivr.net/npm/mermaid@<%= theme.mermaid.version %>/dist/mermaid.min.js"></script>
    <script>mermaid.initialize({
        startOnLoad: <%= theme.mermaid.startOnload %>,
        theme: "<%= theme.mermaid.theme %>"
    });</script>
<% } %>

<%# JS引入 %>
<%- js(['js/mdui.min.js', 'js/meadow.js']) %>